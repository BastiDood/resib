{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "moduleResolution": "Node10",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "noUncheckedIndexedAccess": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
    },
}
